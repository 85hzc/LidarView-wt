<ServerManagerConfiguration>
  <ProxyGroup name="LidarPacketInterpreter">
    <SourceProxy name="WingtechGeneralPacketInterpreter"
         class="vtkWingtechGeneralPacketInterpreter"
         label="Wingtech General Packet Interpreter"
         base_proxygroup="base_LidarPacketInterpreter_g"
         base_proxyname="base_LidarPacketInterpreter">

      <IntVectorProperty
        name="DualReturn"
        command="SetDualReturn"
        default_values="0"
        number_of_elements="1">
        <BooleanDomain name="bool" />
        <Documentation>
          Choose to read only the first acquired point or both returns.
          Most points may be doubled in case this box is checked
        </Documentation>
      </IntVectorProperty>

    </SourceProxy>
  </ProxyGroup>
</ServerManagerConfiguration>
