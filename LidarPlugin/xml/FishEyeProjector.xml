<ServerManagerConfiguration>
  <!-- Begin FishEyeProjector -->
  <ProxyGroup name="filters">
    <SourceProxy name="FishEyeProjector" class="vtkFishEyeProjector" label="FishEyeProjector">
      <Documentation
        short_help="Project 3D points into the image using a FishEye camera model"
        long_help="Project 3D points into the image using a FishEye camera model">
        Project 3D points into the image using a FishEye camera model
      </Documentation>

    <InputProperty
      name="Image"
      port_index="0"
      command="SetInputConnection">
      <DataTypeDomain name="input_type">
        <DataType value="vtkImageData"/>
      </DataTypeDomain>
    </InputProperty>

    <InputProperty
      name="PointCloud"
      port_index="1"
      command="SetInputConnection">
      <DataTypeDomain name="input_type">
        <DataType value="vtkPolyData"/>
      </DataTypeDomain>
    </InputProperty>

    <OutputPort name="Image With Projection" index="0" id="port0" />
    <OutputPort name="Colored Frame" index="1" id="port1" />
    <OutputPort name="Projected Frame" index="2" id="port2" />


    </SourceProxy>
  </ProxyGroup>
  <!-- End FishEyeProjector -->
</ServerManagerConfiguration>
